public class LegacyComponentFactory {

	private LegacyComponentFactory () {}

    private static class LegacySingletonComponentHolder {
        private static final Order INSTANCE = new Order();
    }

    public static Order getInstance() {
        return OrderHolder.INSTANCE;
    }

}
