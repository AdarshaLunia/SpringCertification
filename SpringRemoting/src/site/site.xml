	<bannerLeft>
		<name>BASE Logic: ${project.name}</name>
		<href>index.html</href>
	</bannerLeft>

	<poweredBy>
		<logo img="http://baselogic.com/images/topbar-logo99.png" />
	</poweredBy>


	<!-- <skin> <groupId>org.springframework.maven.skins</groupId> <artifactId>maven-spring-skin</artifactId> 
		<version>1.0.5</version> </skin> -->
	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-fluido-skin</artifactId>
		<version>1.2.1</version>
	</skin>

	<custom>
		<fluidoSkin>
			<topBarEnabled>false</topBarEnabled>
			<sideBarEnabled>true</sideBarEnabled>

			<topBarIcon>
				<name>${project.artifactId}</name>
				<alt>${project.name}</alt>
				<src>http://baselogic.com/images/topbar-logo24.png</src>
				<href>/index.html</href>
			</topBarIcon>

			<googleSearch>
				<sitesearch />
			</googleSearch>

			<googlePlusOne />

			<sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>

			<gitHub>
				<projectId>mickknutson/SpringCertification</projectId>
				<ribbonOrientation>right</ribbonOrientation>
				<ribbonColor>black</ribbonColor>
			</gitHub>

			<!-- <profile>sandbox</profile> -->

			<ohloh>
				<projectId>97428</projectId>
				<widget>factoids</widget>
			</ohloh>

			<twitter>
				<user>mickknutson</user>
				<showUser>true</showUser>
				<showFollowers>true</showFollowers>
			</twitter>

			<facebookLike />

		</fluidoSkin>
	</custom>

	<body>

		<links>
			<item name="${project.name}" href="index.html" />
